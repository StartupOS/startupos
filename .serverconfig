ssl_session_cache shared:SSL:2m;
ssl_session_timeout 5m;

server {
    listen 10.128.0.2:443 default_server ssl;
    server_name _;
    ssl on;
    ssl_certificate /home/jason/startupos/client/startupos.dev/cert1.pem;
    ssl_certificate_key /home/jason/startupos/client/startupos.dev/privkey1.pem;
}

server {
    listen 10.128.0.2:443 ssl;
    server_name jason.startupos.dev;
    location / {
        proxy_pass http://localhost:8081;
    }
}
server {
    listen 10.128.0.2:5000 ssl;
    server_name jason.startupos.dev;
    location / {
        proxy_pass http://localhost:5001;
    }
}

